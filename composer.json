{
    "name": "grechanyuk/comepay",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Egor G.",
            "email": "tech@haters.studio"
        }
    ],
    "homepage": "https://github.com/grechanyuk/comepay",
    "keywords": ["Laravel", "Comepay"],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grechanyuk\\Comepay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grechanyuk\\Comepay\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grechanyuk\\Comepay\\ComepayServiceProvider"
            ],
            "aliases": {
                "Comepay": "Grechanyuk\\Comepay\\Facades\\Comepay"
            }
        }
    }
}
